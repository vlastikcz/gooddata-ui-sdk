{
    "name": "@gooddata/sdk-backend-bear",
    "version": "8.1.0-alpha.44",
    "author": "GoodData",
    "description": "GoodData Backend SPI implementation for the Bear platform",
    "repository": "https://github.com/gooddata/gooddata-ui-sdk/tree/master/libs/sdk-backend-bear",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "esm/index.js",
    "browser": "dist/index.js",
    "typings": "esm/index.d.ts",
    "sideEffects": false,
    "files": [
        "dist/**/*.js",
        "dist/**/*.json",
        "dist/**/*.d.ts",
        "dist/**/*.map",
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map"
    ],
    "config": {
        "eslint": "-c .eslintrc.js --ext ts src/"
    },
    "scripts": {
        "clear-recordings": "rm -rf ./tests/wiremock/recordings/*",
        "start-wiremock": "./tests/wiremock/start_wiremock.sh detached",
        "start-wiremock-recording": "mkdir -p ./tests/wiremock/recordings && ./tests/wiremock/start_wiremock.sh detached && ./tests/wiremock/start_recording.sh",
        "stop-wiremock": "./tests/wiremock/stop_wiremock.sh",
        "clean": "rm -rf ci dist esm coverage *.log && jest --clearCache",
        "dev": "tsc -p tsconfig.dev.json --watch",
        "build": "npm run build-esm",
        "build-all": "concurrently \"npm run build-cjs\" \"npm run build-esm\" && npm run api-extractor",
        "build-cjs": "tsc -p tsconfig.build.json",
        "build-esm": "tsc -p tsconfig.build.esm.json",
        "api-extractor": "mkdir -p api && [ -z \"${CI}\" ] && (api-extractor run -l) || (api-extractor run)",
        "test": "jest --watch",
        "integrated-test": "./scripts/integrated-test.sh",
        "integrated-test-rec": "GD_BEAR_REC=true NODE_TLS_REJECT_UNAUTHORIZED=0 jest --runInBand --config \"integrated-test.config.js\"",
        "refresh-recordings": "npm run clear-recordings && npm run start-wiremock-recording && npm run integrated-test-rec; npm run stop-wiremock",
        "test-once": "./scripts/test-once.sh",
        "test-ci": "./scripts/test-ci.sh",
        "eslint": "eslint $npm_package_config_eslint",
        "eslint-ci": "mkdir -p ./ci/results && eslint -f checkstyle -o ci/results/eslint-results.xml $npm_package_config_eslint",
        "prettier-check": "prettier --check '{src,test}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write '{src,test}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.js --output-type err-long src/",
        "dep-cruiser-ci": "depcruise --validate .dependency-cruiser.js --output-type err-long src/",
        "validate": "npm run dep-cruiser && npm run eslint && npm run prettier-check",
        "validate-ci": "npm run dep-cruiser-ci && npm run eslint-ci && npm run prettier-check"
    },
    "dependencies": {
        "@gooddata/api-client-bear": "^8.1.0-alpha.44",
        "@gooddata/api-model-bear": "^8.1.0-alpha.44",
        "@gooddata/sdk-backend-base": "^8.1.0-alpha.44",
        "@gooddata/sdk-backend-spi": "^8.1.0-alpha.44",
        "@gooddata/sdk-embedding": "^8.1.0-alpha.44",
        "@gooddata/sdk-model": "^8.1.0-alpha.44",
        "date-fns": "^2.8.1",
        "http-status-codes": "^1.3.0",
        "json-stable-stringify": "^1.0.1",
        "lodash": "^4.17.19",
        "spark-md5": "^3.0.0",
        "ts-invariant": "^0.4.4",
        "tslib": "^2.0.0",
        "uuid": "^3.3.3"
    },
    "devDependencies": {
        "@gooddata/eslint-config": "^2.0.0",
        "@gooddata/reference-workspace": "^8.1.0-alpha.44",
        "@gooddata/sdk-backend-mockingbird": "^8.1.0-alpha.44",
        "@microsoft/api-extractor": "^7.3.8",
        "@types/jest": "^26.0.12",
        "@types/json-stable-stringify": "^1.0.32",
        "@types/lodash": "^4.14.158",
        "@types/spark-md5": "^3.0.1",
        "@types/uuid": "^3.4.5",
        "@typescript-eslint/eslint-plugin": "^3.7.1",
        "@typescript-eslint/parser": "^3.7.1",
        "concurrently": "^5.3.0",
        "dependency-cruiser": "^9.9.5",
        "dotenv": "^8.2.0",
        "eslint": "^7.5.0",
        "eslint-plugin-header": "^3.0.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jest": "^23.20.0",
        "eslint-plugin-prettier": "^3.1.4",
        "jest": "^26.4.2",
        "jest-junit": "^3.0.0",
        "prettier": "~2.0.5",
        "ts-jest": "^26.3.0",
        "typescript": "4.0.2"
    }
}
